{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/login/login",
			"style": {
				"navigationStyle": "custom" // 去掉页头
			}
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/terminal/terminal",
			"style": {
				"navigationBarTitleText": "数据终端",
				"navigationBarTextStyle":"white",
				"navigationBarBackgroundColor":" #3366CC",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/developer/developer",
			"style": {
				"navigationBarTitleText": "开发者",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/user/user",
			"style": {
				"navigationBarTitleText": "用户中心",
				"enablePullDownRefresh": false
			}

		}
        ,{
            "path" : "pages/user/account/accountManagement",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "账户管理",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/user/account/edituser/editUserName",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "修改用户名",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/user/account/edituser/editUserPhone",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "修改手机号码",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/user/account/edituser/editUserEmail",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "修改邮箱",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/user/account/edituser/editUserPassword",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "修改密码",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/user/account/edituser/edituserphonenext/editUserPhoneNext",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "修改手机号码",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/user/account/edituser/edituseremailnext/editUserEmailNext",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "修改邮箱",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/user/journal/journal",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "系统日志",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/terminal/EditTerminal",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "详细信息",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#007aff",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/images/index.png",
			"selectedIconPath": "static/images/index-color.png",
			"text": "首页"
		}, {
			"pagePath": "pages/terminal/terminal",
			"iconPath": "static/images/terminal.png",
			"selectedIconPath": "static/images/terminal-color.png",
			"text": "数据终端"
		}, {
			"pagePath": "pages/developer/developer",
			"iconPath": "static/images/developer.png",
			"selectedIconPath": "static/images/developer-color.png",
			"text": "开发者"
		}, {
			"pagePath": "pages/user/user",
			"iconPath": "static/images/user.png",
			"selectedIconPath": "static/images/user-color.png",
			"text": "用户中心"
		}]
	}
